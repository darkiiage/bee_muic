<% provide(:title, "Edit Event Types") %>
<h1>Update Event Types : <%= @event_type.name %> </h1>

<div class = "row">
  <div class="span4 offset2">
	<%= form_for(@event_type) do |f| %>

	  <%= f.label :name %>
	  <%= f.text_field :name %>
	  
	  <%= f.label :image %>
	  <%= f.file_field :image%>

	  <%= f.submit "Edit this Event Type", class: "btn btn-medium btn-warning" %>
	<% end %>
  </div>
</div>